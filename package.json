{
  "name": "friendconnector",
  "version": "1.0.0",
  "description": "Social Network For Friends",
  "main": "server.js",
  "engines": {
    "node": "12.13.0",
    "npm": "6.x"
  },
  "scripts": {
    "start": "if-env NODE_ENV='production' && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon server.js --ignore 'client/*'\" \"npm run client\" ",
    "cd": "cd client && npm install",
    "client": "cd client && npm run start",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anish732/Project-3.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anish732/Project-3/issues"
  },
  "homepage": "https://github.com/anish732/Project-3#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.2.4",
    "express": "^4.17.3",
    "express-validator": "^6.3.0",
    "gravatar": "^1.8.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.13",
    "request": "^2.88.0",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "nodemon": "^2.0.1"
  }
}
